{
  "expo": {
    "name": "MoveMate",
    "slug": "movemate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
     "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#6A458F",
          "image": "./assets/icon.png",
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#6A458F"
          },
          "imageWidth": 200
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A458F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.covenantcodes.movemate"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6fcc3706-a733-463e-ab76-4b093a4846bc"
      }
    },
    "owner": "covenantcodes"
  }
}
